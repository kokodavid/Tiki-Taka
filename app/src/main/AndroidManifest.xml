<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tiki_taka">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/kud"
        android:label="@string/app_name"
        android:roundIcon="@drawable/kud"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.SavedEventListActivity"></activity>
        <activity
            android:name=".ui.SignupActivity"
            android:theme="@style/NoActionBarTheme" />
        <activity android:name=".ui.EventDetailActivity" />
        <activity android:name=".ui.FirstActivity" />
        <activity
            android:name=".ui.LoginActivity"
            android:theme="@style/NoActionBarTheme" />
        <activity android:name=".ui.detailsactivity" />
        <activity android:name=".ui.TicketActivity" />
        <activity android:name=".ui.HomeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tiki_taka.ui.HomeActivity" />
        </activity>
        <activity
            android:name=".ui.MainActivity"
            android:theme="@style/NoActionBarTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>